<main class="container">
  <h1>Posts</h1>
  <div>
  <%= turbo_frame_tag "new_post_frame" do %>
    <h2><%= link_to "New post", new_post_path, data: { turbo_stream: true, turbo_frame: "new_post_frame" } %></h2>
  <% end %>
  </div>
  <div>
  <%= turbo_frame_tag "posts_frame" do %>
    <%= render @posts %>
  <% end %>
  </div>
</main>
